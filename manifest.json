{
    "manifest_version": 3,
    "name": "RBean Tricks",
    "description": "Share RBean Token. Private app for 2600 students",
    "version": "1.0.0",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-32.png",
        "128": "logo/logo-32.png"
    },
    "action": {
        "default_title": "RBean Token"
    },
    "host_permissions": [
        "*://*.yohangastoud.fr/*"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [
        {
            "js": [
                "foreground.js"
            ],
            "matches": [
                "https://2600.rbean.io/*"
            ]
        }
    ]
}